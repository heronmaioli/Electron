      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "electron": "^9.0.4",
    "electron-builder": "^22.7.0",
    "morgan": "^1.10.0",
    "wait-on": "^5.0.1"
  }
